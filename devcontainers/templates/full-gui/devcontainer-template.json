{
  "$schema": "https://aka.ms/devcontainer-template-schema",
  "id": "full-gui",
  "version": "0.1.0",
  "name": "AirNub full GUI workspace",
  "publisher": "airnub-labs",
  "description": "Extends the base workspace with GUI tooling, Docker-in-Docker helpers, and Codespaces friendly forwarding.",
  "documentationURL": "https://github.com/airnub-labs/vscode-meta-workspace-internal/blob/main/docs/workspace-architecture.md",
  "licenseURL": "https://github.com/airnub-labs/vscode-meta-workspace-internal/blob/main/LICENCE",
  "platforms": [
    "linux/amd64"
  ],
  "dockerComposeFile": [
    "../base/.devcontainer/docker-compose.yml",
    ".devcontainer/docker-compose.override.yml"
  ],
  "features": {
    "ghcr.io/devcontainers/features/common-utils:2": {
      "username": "vscode",
      "upgradePackages": false
    },
    "ghcr.io/devcontainers/features/node:1": {
      "version": "24",
      "pnpmVersion": "10.0.0",
      "nodeGypDependencies": true
    },
    "ghcr.io/devcontainers/features/python:1": {
      "version": "3.12"
    },
    "ghcr.io/airnub-labs/devcontainers/features/agent-tooling-clis:latest": {
      "installCodex": true,
      "installClaude": true,
      "installGemini": false
    },
    "ghcr.io/devcontainers/features/docker-in-docker:2": {
      "moby": true,
      "version": "latest"
    },
    "ghcr.io/airnub-labs/devcontainers/features/docker-in-docker-helpers:latest": {},
    "ghcr.io/airnub-labs/devcontainers/features/gui-tooling:latest": {}
  }
}
