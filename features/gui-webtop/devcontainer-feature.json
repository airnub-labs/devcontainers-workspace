  "documentationURL": "https://github.com/airnub-labs/vscode-meta-workspace-internal/blob/main/docs/gui-providers.md",
  "options": {
    "audio": {
      "type": "boolean",
      "default": true,
      "description": "Enable the built-in PulseAudio bridge."
    }
  },
  "installsAfter": [
    "ghcr.io/devcontainers/features/common-utils"
  ]
}
